<model>
    <mount>//OPNsense/zerotier</mount>
    <description>
        Zerotier - Virtual Networks That Just Work.
    </description>
    <version>1.3.0</version>
    <items>
        <enabled type="BooleanField">
            <default>0</default>
            <Required>Y</Required>
        </enabled>
        <apiAccessToken type="TextField">
            <default></default>
            <Required>N</Required>
        </apiAccessToken>
        <localconf type="TextField">
            <Required>N</Required>
        </localconf>
        <networks>
            <network type="ArrayField">
                <enabled type="BooleanField">
                    <default>0</default>
                    <Required>Y</Required>
                </enabled>
                <networkId type="TextField">
                    <default></default>
                    <Required>Y</Required>
                </networkId>
                <description type="TextField">
                    <default></default>
                    <Required>N</Required>
                </description>
            </network>
        </networks>
    </items>
</model>
