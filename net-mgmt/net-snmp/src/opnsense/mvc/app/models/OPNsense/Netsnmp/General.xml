<model>
    <mount>//OPNsense/netsnmp/general</mount>
    <description>Netsnmp configuration</description>
    <version>1.0.4</version>
    <items>
        <enabled type="BooleanField">
            <default>0</default>
            <Required>Y</Required>
        </enabled>
        <community type="TextField">
            <default></default>
            <Required>N</Required>
        </community>
        <syslocation type="TextField">
            <default></default>
            <Required>N</Required>
        </syslocation>
        <syscontact type="TextField">
            <default></default>
            <Required>N</Required>
        </syscontact>
        <l3visibility type="BooleanField">
            <default>0</default>
            <Required>Y</Required>
        </l3visibility>
        <versionoid type="BooleanField">
            <default>0</default>
            <Required>Y</Required>
        </versionoid>
        <enableagentx type="BooleanField">
            <default>0</default>
            <Required>Y</Required>
        </enableagentx>
        <listen type="NetworkField">
            <FieldSeparator>,</FieldSeparator>
            <Required>N</Required>
            <asList>Y</asList>
        </listen>
        <listen type="snmpdflags">
            <default>0</default>
            <Required>Y</Required>
        </listen>
        <listen type="trapenabled">
            <default>0</default>
            <Required>Y</Required>
        </listen>
        <listen type="snmptrapdflags">
            <default>0</default>
            <Required>Y</Required>
        </listen>
        <listen type="trapzabbix">
            <default>0</default>
            <Required>Y</Required>
        </listen>
    </items>
</model>
