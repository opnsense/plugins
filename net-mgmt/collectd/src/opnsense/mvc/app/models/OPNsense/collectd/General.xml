<model>
    <mount>//OPNsense/collectd/general</mount>
    <description>collectd configuration</description>
    <items>
        <enabled type="BooleanField">
            <default>0</default>
            <Required>Y</Required>
        </enabled>
        <hostname type="TextField">
            <default></default>
            <Required>Y</Required>
        </hostname>
        <fqdnlookup type="BooleanField">
            <default>0</default>
            <Required>N</Required>
        </fqdnlookup>
        <interval type="IntegerField">
            <default>10</default>
            <Required>N</Required>
        </interval>
    </items>
</model>
